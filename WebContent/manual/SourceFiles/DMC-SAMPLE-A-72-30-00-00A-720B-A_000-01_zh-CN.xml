  <content>
    <proced>
      <prelreqs>
        <reqconds>
          <reqcond>打开反推门。</reqcond>
        </reqconds>
        <supequip>
          <supeqli>
            <supequi>
              <nomen>维护平台,1号和3号发动机</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty>1</qty>
              <remarks>高 4-8 ft(1.20-2.40 m)</remarks>
            </supequi>
            <supequi>
              <nomen>高空作业台,2号发动机</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty>1</qty>
            </supequi>
            <supequi>
              <nomen>扭矩扳手</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty>1</qty>
              <remarks>力矩为 0-600 in-lb (0-68.0 N.m)</remarks>
            </supequi>
            <supequi>
              <nomen>卷缩机</nomen>
              <identno>
                <mfc></mfc>
                <pnr>M303,M305</pnr>
              </identno>
              <qty></qty>
              <remarks>Bergen 机械的(选择性的)</remarks>
            </supequi>
          </supeqli>
        </supequip>
        <supplies>
          <supplyli>
            <supply>
              <nomen>润滑剂</nomen>
              <identno>
                <mfc></mfc>
                <pnr>PWA 36001</pnr>
              </identno>
              <qty>1</qty>
            </supply>
            <supply>
              <nomen>溶剂,石油 PD 680类型 I</nomen>
              <identno>
                <mfc></mfc>
                <pnr>PMC 9001</pnr>
              </identno>
              <qty>1</qty>
            </supply>
            <supply>
              <nomen>保险丝</nomen>
              <identno>
                <mfc></mfc>
                <pnr>AS3214-02</pnr>
              </identno>
              <qty>1</qty>
            </supply>
            <supply>
              <nomen>保险电缆</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty></qty>
              <remarks>选择性的</remarks>
            </supply>
            <supply>
              <nomen>保险电缆环</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty></qty>
            </supply>
            <supply>
              <nomen></nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
              <qty></qty>
              <remarks>选择性的</remarks>
            </supply>
            <supply>
              <nomen>键垫圈</nomen>
              <identno>
                <mfc></mfc>
                <pnr></pnr>
              </identno>
