    <descript>
      <figure id="fig-001">
        <title>上侧显示组件</title>
        <graphic boardno="ICN-SAMPLE-A-720000-A-SBTR-00021-A-01-1" xlink:type="simple" reprohgt="69" reprowid="150">
          <hotspot visibility="visible" apsid="AUTOID_8726" apsname="1" id="hot-001"></hotspot>
          <hotspot visibility="visible" apsid="AUTOID_8729" apsname="2" id="hot-002"></hotspot>
          <hotspot visibility="visible" apsid="AUTOID_8732" apsname="3" id="hot-003"></hotspot>
